{
  "id": "/hello-dcos",
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "aayushjain/playground:latest",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "labels": {
            "VIP_0": "/hello-dcos:8080"
          },
          "protocol": "tcp",
          "name": "hellodcos"
        }
      ]
    }
  },
  "cpus": 1,
  "mem": 1024,
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "api.airtrumpet.cloud"
  }
}